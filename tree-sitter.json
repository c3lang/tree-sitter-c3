{
  "grammars": [
    {
      "name": "c3",
      "camelcase": "C3",
      "scope": "source.c3",
      "path": ".",
      "file-types": [
        "c3",
        "c3i",
        "c3t"
      ],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "^c3$"
    }
  ],
  "metadata": {
    "version": "0.8.0",
    "license": "MIT",
    "description": "C3 grammar for node-tree-sitter",
    "authors": [
      {
        "name": "Christian Buttner"
      }
    ],
    "links": {
      "repository": "https://github.com/c3lang/tree-sitter-c3"
    }
  },
  "bindings": {
    "c": true,
    "c3": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
