{
  "name": "tree-sitter-c3",
  "version": "0.8.2",
  "description": "C3 grammar for tree-sitter",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c3lang/tree-sitter-c3.git"
  },
  "funding": "",
  "license": "MIT",
  "author": {
    "name": "Christian Buttner",
    "url": ""
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "c3"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "git+https://github.com/tree-sitter/node-tree-sitter.git",
    "tree-sitter-cli": "^0.26.5"
  },
  "peerDependencies": {
    "tree-sitter": "git+https://github.com/tree-sitter/node-tree-sitter.git"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "check:stdlib": "node scripts/check-stdlib-grammar.mjs",
    "check:c3c-test": "node scripts/check-stdlib-grammar.mjs --std-dir /Users/f00lg/github/c3/c3c/test",
    "check:queries": "npx tree-sitter-cli query \"queries/highlights.scm\" \"test/corpus/expression.txt\" --quiet && npx tree-sitter-cli query \"queries/highlights.scm\" \"test/corpus/statement.txt\" --quiet"
  }
}
